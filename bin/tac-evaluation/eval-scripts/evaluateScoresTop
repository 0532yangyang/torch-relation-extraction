year=$1
prediction=$2
exampleDir=$3
headNum=$4
outReport=$5
export TAC_ROOT=/iesl/canvas/belanger/relationfactory

headNum=`echo $headNum | sed 's|\..*||'`
tmpPrediction=`mktemp`
echo $tmpPrediction
cp $prediction $tmpPrediction.1

sort -t$'\t' -k9 -nr   $tmpPrediction.1 | head -n $headNum > $tmpPrediction
sh /home/pat/canvas/universal-schema/univSchema/torch/david-evaluation/evaluate${year} $tmpPrediction $exampleDir $outReport  

rm $tmpPrediction*
